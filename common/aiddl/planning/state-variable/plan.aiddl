(#mod self org.aiddl.common.planning.state-variable.plan)

(#req EVAL org.aiddl.eval.namespace)
(#nms E basic@EVAL)

(#req FL org.aiddl.common.reasoning.logic.first-order)

(#def SequentialPlan
  (and
    (has-type #self ^list)
    (forall ?e #self (has-type ?e ^Atom@FL))))

(#def Plan
  (or
    (= #self NIL)
    (has-type #self ^$SequentialPlan)
    ;;(has-type #self PartiallyOrderedPlan)
    ;;(has-type #self ParallelPlan)
))