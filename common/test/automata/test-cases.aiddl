(#mod self org.aiddl.automata.test)

(#req A org.aiddl.common.automata.discrete-fininte-state-machine)


(#assert t1
  (org.aiddl.test
    (true
      (org.aiddl.def
        (type
          (
            {s1 s2}
            {a b}
            {
              (s1 a) : s1
              (s1 b) : s2
              (s2 b) : s2
            }
            s1
            {s2}
          ) DFA@A )))))

