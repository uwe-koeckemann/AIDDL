(#mod self org.aiddl.automata.test)

(#req EVAL org.aiddl.eval.namespace)
(#nms E basic@EVAL)

(#req A org.aiddl.common.automata.discrete-finite-state-machine)

(#assert t1
  (has-type
    (
      {s1 s2}
      {a b}
      {
        (s1 a) : s1
        (s1 b) : s2
        (s2 b) : s2
      }
      s1
      {s2}
    ) ^DFA@A ))

(#assert t2
  (not
    (has-type
      (
        {s1 s2}
        {a b}
        {
          (s1 a) : s1
          (s1 b) : s2
          (s2 b) : s2
          (s2 b) : s3
        }
        s1
        {s2}
      ) ^DFA@A )))

(#assert t3
  (not
     (has-type
       (
         {s1 s2}
         {a b}
         {
           (s1 a) : s1
           (s1 b) : s2
           (s2 b) : s2
           (s3 b) : s2
         }
         s1
         {s2}
       ) ^DFA@A )))


(#assert t4
  (not
    (has-type
      (
        {s1 s2}
        {a b}
        {
          (s1 a) : s1
          (s1 b) : s2
          (s2 b) : s2
          (s2 b) : s2
        }
        s3
        {s2}
      ) ^DFA@A )))


(#assert t5
  (not
    (has-type
      (
        {s1 s2}
        {a b}
        {
          (s1 a) : s1
          (s1 b) : s2
          (s2 b) : s2
          (s2 b) : s2
        }
        s1
        {s3}
      ) ^DFA@A )))

(#assert t6
  (not
    (has-type
      (
        {s1 s2}
        {a b}
        {
          (s1 a) : s1
          (s1 b) : s2
          (s2 b) : s2
          (s2 c) : s2
        }
        s1
        {s2}
      ) ^DFA@A )))
        