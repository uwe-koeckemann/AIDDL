(#mod self org.aiddl.core.test.eval.math)

(#req Math org.aiddl.eval.math)

(#nms E org.aiddl.eval)
(#nms M (nms functions@Math))

(#assert t-trig
  [
    (= (deg2rad (rad2deg PI@Math)) PI@Math)
  ])

(#assert t-expt
  [
    (= (expt 2 0) 1)
    (= (expt 2 1) 2)
    (= (expt 2 2) 4)
    (= (expt 2 3) 8)
    (= (expt 1/4 2) 1/16)
  ])


(#assert t-abs
  [
    (= (abs -10) 10)
    (= (abs  10) 10)
  ])
